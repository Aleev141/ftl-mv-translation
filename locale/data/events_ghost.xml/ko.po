msgid ""
"data/events_ghost.xml$/FTL/event[@name=\"NEBULA_GHOST_FEDMORALITY\"]/choice[1]/event/choice/event/choice[1]/event/choice[3]/text"
msgstr "Attack one of the Zoltan wrecks."

#: src-en/data/events_ghost.xml:2654
msgid ""
"data/events_ghost.xml$/FTL/event[@name=\"NEBULA_GHOST_FEDMORALITY\"]/choice[1]/event/choice/event/choice[1]/event/choice[3]/event/text"
msgstr ""
