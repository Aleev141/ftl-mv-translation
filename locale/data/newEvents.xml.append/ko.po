#: src-en/data/newEvents.xml.append:1619
msgid "data/newEvents.xml.append$/FTL/event[@name=\"REFUGEE_NO_DISTRESS\"]/text"
msgstr ""
"Your sensors have picked up a refugee ship drifting through the system, no "
"doubt one of many fleeing the Rebel advance. It doesn't appear to have "
"detected you... or else it is trying to avoid notice."

#: src-en/data/newEvents.xml.append:1621
msgid ""
"data/newEvents.xml.append$/FTL/event[@name=\"REFUGEE_NO_DISTRESS\"]/choice[1]/text"
msgstr "Hail them."

#: src-en/data/newEvents.xml.append:1625
msgid ""
"data/newEvents.xml.append$/FTL/event[@name=\"REFUGEE_NO_DISTRESS\"]/choice[2]/text"
msgstr "Ignore the refugees."

#: src-en/data/newEvents.xml.append:1627
msgid ""
"data/newEvents.xml.append$/FTL/event[@name=\"REFUGEE_NO_DISTRESS\"]/choice[2]/event/text"
msgstr "You decide to ignore the refugees."

#: src-en/data/newEvents.xml.append:1629
msgid ""
"data/newEvents.xml.append$/FTL/event[@name=\"REFUGEE_NO_DISTRESS\"]/choice[2]/event/choice[1]/text"
msgstr "Check the storage."

#: src-en/data/newEvents.xml.append:1633
msgid ""
"data/newEvents.xml.append$/FTL/event[@name=\"REFUGEE_NO_DISTRESS\"]/choice[2]/event/choice[2]/text"
msgstr "Do Nothing."

#: src-en/data/newEvents.xml.append:1645
msgid ""
"data/newEvents.xml.append$/FTL/eventList[@name=\"REFUGEE_HAIL_LIST\"]/event[5]/text"
msgstr ""
