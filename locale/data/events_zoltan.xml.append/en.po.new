msgid ""
"data/events_zoltan.xml.append$//event[@name=\"ZOLTAN_QUEST_LAWBOOK_PIRATE\"]/choice[3]/event/choice[1]/event/choice/text"
msgstr "Continue..."

#: src-en/data/events_zoltan.xml.append:1014
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"ZOLTAN_QUEST_LAWBOOK_PIRATE\"]/choice[3]/event/choice[2]/text"
msgstr "Just fight them for it."

#: src-en/data/events_zoltan.xml.append:1016
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"ZOLTAN_QUEST_LAWBOOK_PIRATE\"]/choice[3]/event/choice[2]/event/text"
msgstr "You aren't paying for the damn book. You'll just take it from them."

#: src-en/data/events_zoltan.xml.append:1019
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"ZOLTAN_QUEST_LAWBOOK_PIRATE\"]/choice[3]/event/choice[2]/event/choice/text"
msgstr "Continue..."

#: src-en/data/events_zoltan.xml.append:1025
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"ZOLTAN_QUEST_LAWBOOK_PIRATE\"]/choice[3]/event/choice[3]/text"
msgstr "No thanks."

#: src-en/data/events_zoltan.xml.append:1027
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"ZOLTAN_QUEST_LAWBOOK_PIRATE\"]/choice[3]/event/choice[3]/event/text"
msgstr "The Smugglers don't attempt to negotiate further."

#: src-en/data/events_zoltan.xml.append:1029
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"ZOLTAN_QUEST_LAWBOOK_PIRATE\"]/choice[3]/event/choice[3]/event/choice[1]/text"
msgstr "Do something on-board the ship."

#: src-en/data/events_zoltan.xml.append:1033
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"ZOLTAN_QUEST_LAWBOOK_PIRATE\"]/choice[3]/event/choice[3]/event/choice[2]/text"
msgstr "Do nothing."

#: src-en/data/events_zoltan.xml.append:1042
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"ZOLTAN_QUEST_LAWBOOK_DUSKBRINGER\"]/text"
msgstr ""
"You notice a Duskbringer ship collecting the lawbook from a pirate vessel. "
"You'll likely have to fight them for it if you want it back."

#: src-en/data/events_zoltan.xml.append:1045
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"ZOLTAN_QUEST_LAWBOOK_DUSKBRINGER\"]/choice[1]/text"
msgstr "Take back the law book."

#: src-en/data/events_zoltan.xml.append:1047
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"ZOLTAN_QUEST_LAWBOOK_DUSKBRINGER\"]/choice[1]/event/text"
msgstr "You prepare to engage the Duskbringer ship."

#: src-en/data/events_zoltan.xml.append:1050
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"ZOLTAN_QUEST_LAWBOOK_DUSKBRINGER\"]/choice[1]/event/choice/text"
msgstr "Continue..."

#: src-en/data/events_zoltan.xml.append:1056
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"ZOLTAN_QUEST_LAWBOOK_DUSKBRINGER\"]/choice[2]/text"
msgstr "Leave."

#: src-en/data/events_zoltan.xml.append:1058
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"ZOLTAN_QUEST_LAWBOOK_DUSKBRINGER\"]/choice[2]/event/text"
msgstr "You aren't interested in fighting the Duskbringer ship."

#: src-en/data/events_zoltan.xml.append:1060
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"ZOLTAN_QUEST_LAWBOOK_DUSKBRINGER\"]/choice[2]/event/choice[1]/text"
msgstr "Do something on-board the ship."

#: src-en/data/events_zoltan.xml.append:1064
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"ZOLTAN_QUEST_LAWBOOK_DUSKBRINGER\"]/choice[2]/event/choice[2]/text"
msgstr "Do nothing."

#: src-en/data/events_zoltan.xml.append:1071
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"ZOLTAN_QUEST_LAWBOOK_ENGI\"]/text"
msgstr ""
"An Engi ship at this beacon is sitting dormant. You hail it, asking if it "
"has seen the lost law book. \"Bound paper object of conduction code has been"
" found. Intentions: keep book safe until owner returns.\""

#: src-en/data/events_zoltan.xml.append:1074
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"ZOLTAN_QUEST_LAWBOOK_ENGI\"]/choice[1]/text"
msgstr "Tell the Engi that the Zoltans sent you."

#: src-en/data/events_zoltan.xml.append:1076
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"ZOLTAN_QUEST_LAWBOOK_ENGI\"]/choice[1]/event/text"
msgstr ""
"\"Likelihood of Zoltans sending Federation ship to retrieve Law Book... "
"favorable.\" They send over the book."

#: src-en/data/events_zoltan.xml.append:1078
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"ZOLTAN_QUEST_LAWBOOK_ENGI\"]/choice[1]/event/choice/text"
msgstr "Continue..."

#: src-en/data/events_zoltan.xml.append:1084
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"ZOLTAN_QUEST_LAWBOOK_ENGI\"]/choice[2]/text"
msgstr "Just fight them for it."

#: src-en/data/events_zoltan.xml.append:1086
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"ZOLTAN_QUEST_LAWBOOK_ENGI\"]/choice[2]/event/text"
msgstr "You prepare to engage the Engi ship."

#: src-en/data/events_zoltan.xml.append:1089
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"ZOLTAN_QUEST_LAWBOOK_ENGI\"]/choice[2]/event/choice/text"
msgstr "Continue..."

#: src-en/data/events_zoltan.xml.append:1095
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"ZOLTAN_QUEST_LAWBOOK_ENGI\"]/choice[3]/text"
msgstr "Leave."

#: src-en/data/events_zoltan.xml.append:1097
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"ZOLTAN_QUEST_LAWBOOK_ENGI\"]/choice[3]/event/text"
msgstr ""
"The law book is likely safe with the Engi. Hopefully, the Zoltans return "
"here to find the Engi and get the book back."

#: src-en/data/events_zoltan.xml.append:1099
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"ZOLTAN_QUEST_LAWBOOK_ENGI\"]/choice[3]/event/choice[1]/text"
msgstr "Do something on-board the ship."

#: src-en/data/events_zoltan.xml.append:1103
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"ZOLTAN_QUEST_LAWBOOK_ENGI\"]/choice[3]/event/choice[2]/text"
msgstr "Do nothing."

#: src-en/data/events_zoltan.xml.append:1111
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"ZOLTAN_QUEST_LAWBOOK_OBTAIN\"]/text"
msgstr ""
"Now that you have the lawbook, you can head back to the location of the "
"Zoltan ship."

#: src-en/data/events_zoltan.xml.append:1116
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"ZOLTAN_QUEST_LAWBOOK_RETURN\"]/text"
msgstr ""
"You've found the Zoltan ship again. You send over the law book, for which "
"they are extremely grateful. They give you an augment and some supplies."

#: src-en/data/events_zoltan.xml.append:1143
msgid "data/events_zoltan.xml.append$//event[@name=\"NEBULA_ZOLTAN_ROCK\"]/text"
msgstr ""
"This nebula turns out to be the hiding place of a terrified Rock crew taking"
" refuge from the Zoltan border police. They don't seem prepared to risk your"
" leaving with their co-ordinates, and open fire!"

#: src-en/data/events_zoltan.xml.append:1147
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"NEBULA_ZOLTAN_ROCK\"]/choice/text"
msgstr "Continue..."

#: src-en/data/events_zoltan.xml.append:1153
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"STORM_ZOLTAN_SUPPLY_CHOICE\"]/text"
msgstr ""
"You spy two pirate ships lurking in the nebula here. They remain unaware of "
"your presence; you're able to get your scanners to at least identify their "
"cargo: One is carrying the fuel supplies, the other the ammunition. They "
"begin to drift away from each other in the storm."

#: src-en/data/events_zoltan.xml.append:1156
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"STORM_ZOLTAN_SUPPLY_CHOICE\"]/choice[1]/text"
msgstr "Secure the fuel supply."

#: src-en/data/events_zoltan.xml.append:1158
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"STORM_ZOLTAN_SUPPLY_CHOICE\"]/choice[1]/event/text"
msgstr ""
"You jet toward the pirate with the fuel supplies and engage - hopefully you "
"can leave the ship in one piece!"

#: src-en/data/events_zoltan.xml.append:1161
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"STORM_ZOLTAN_SUPPLY_CHOICE\"]/choice[1]/event/choice/text"
msgstr "Continue..."

#: src-en/data/events_zoltan.xml.append:1167
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"STORM_ZOLTAN_SUPPLY_CHOICE\"]/choice[2]/text"
msgstr "Secure the ammunition."

#: src-en/data/events_zoltan.xml.append:1169
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"STORM_ZOLTAN_SUPPLY_CHOICE\"]/choice[2]/event/text"
msgstr ""
"You jet toward the pirate with the ammunition and engage - hopefully you can"
" leave the ship in one piece!"

#: src-en/data/events_zoltan.xml.append:1172
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"STORM_ZOLTAN_SUPPLY_CHOICE\"]/choice[2]/event/choice/text"
msgstr "Continue..."

#: src-en/data/events_zoltan.xml.append:1178
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"STORM_ZOLTAN_SUPPLY_CHOICE\"]/choice[3]/text"
msgstr "Let them leave."

#: src-en/data/events_zoltan.xml.append:1180
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"STORM_ZOLTAN_SUPPLY_CHOICE\"]/choice[3]/event/text"
msgstr "Sometimes discretion is the better part of valor."

#: src-en/data/events_zoltan.xml.append:1182
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"STORM_ZOLTAN_SUPPLY_CHOICE\"]/choice[3]/event/choice[1]/text"
msgstr "Do something onboard the ship."

#: src-en/data/events_zoltan.xml.append:1186
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"STORM_ZOLTAN_SUPPLY_CHOICE\"]/choice[3]/event/choice[2]/text"
msgstr "Do nothing."

#: src-en/data/events_zoltan.xml.append:1233
msgid "data/events_zoltan.xml.append$//event[@name=\"CAPITAL_FREE_REACTOR\"]/text"
msgstr ""
"You're greeted by a ship full of Zoltan Engineers, who out of kindness offer"
" to upgrade your reactor for free."

#: src-en/data/events_zoltan.xml.append:1238
msgid "data/events_zoltan.xml.append$//event[@name=\"CAPITAL_SMUGGLER\"]/text"
msgstr ""
"A smuggler ship hails you. \"Greetings soon-to-be friends! We've recently "
"been having some problems with the Zoltan police, who do not particularly "
"enjoy our business. Would you care to trade, so that we may offload some "
"supplies and get out of this sector faster?\""

#: src-en/data/events_zoltan.xml.append:1240
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_SMUGGLER\"]/choice[1]/text"
msgstr "Agree to trade."

#: src-en/data/events_zoltan.xml.append:1244
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_SMUGGLER\"]/choice[2]/text"
msgstr "Decline."

#: src-en/data/events_zoltan.xml.append:1246
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_SMUGGLER\"]/choice[2]/event/text"
msgstr ""
"If the Smugglers are selling contraband that the Zoltans want rid of, then "
"it's best we don't associate ourselves with them."

#: src-en/data/events_zoltan.xml.append:1248
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_SMUGGLER\"]/choice[2]/event/choice[1]/text"
msgstr "Do something onboard the ship."

#: src-en/data/events_zoltan.xml.append:1252
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_SMUGGLER\"]/choice[2]/event/choice[2]/text"
msgstr "Do nothing."

#: src-en/data/events_zoltan.xml.append:1266
msgid ""
"data/events_zoltan.xml.append$//eventList[@name=\"CAPITAL_SMUGGLER_TRADE\"]/event[3]/text"
msgstr ""
"Suddenly, a Zoltan ship jumps out of nowhere! Frightened, the Smugglers jump"
" away, leaving you to deal with the attackers."

#: src-en/data/events_zoltan.xml.append:1269
msgid ""
"data/events_zoltan.xml.append$//eventList[@name=\"CAPITAL_SMUGGLER_TRADE\"]/event[3]/choice/text"
msgstr "Continue..."

#: src-en/data/events_zoltan.xml.append:1276
msgid "data/events_zoltan.xml.append$//event[@name=\"CAPITAL_CREW_STUDY\"]/text"
msgstr ""
"You arrive at a Zoltan research facility. They say they are researching "
"genetic distortion due to stasis sleep and prolonged FTL travel. They ask if"
" your crew has the time to undergo a few scans."

#: src-en/data/events_zoltan.xml.append:1278
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_CREW_STUDY\"]/choice[1]/text"
msgstr "Participate in their study."

#: src-en/data/events_zoltan.xml.append:1282
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_CREW_STUDY\"]/choice[2]/text"
msgstr "Decline."

#: src-en/data/events_zoltan.xml.append:1284
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_CREW_STUDY\"]/choice[2]/event/text"
msgstr "\"Alright. Fly safe.\" You prepare to leave."

#: src-en/data/events_zoltan.xml.append:1286
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_CREW_STUDY\"]/choice[2]/event/choice[1]/text"
msgstr "Do something onboard the ship."

#: src-en/data/events_zoltan.xml.append:1290
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_CREW_STUDY\"]/choice[2]/event/choice[2]/text"
msgstr "Do nothing."

#: src-en/data/events_zoltan.xml.append:1296
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_CREW_STUDY\"]/choice[@req=\"medbay\"]/text"
msgstr "(Adv. Medbay) Give them your medical records."

#: src-en/data/events_zoltan.xml.append:1298
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_CREW_STUDY\"]/choice[@req=\"medbay\"]/event/text"
msgstr ""
"\"Thank you! We didn't expect to receive such a significant amount of data "
"regarding your crew's health during FTL travel. Please, accept this for your"
" trouble.\""

#: src-en/data/events_zoltan.xml.append:1304
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_CREW_STUDY\"]/choice[@req=\"LIST_CREW_SHELL\"]/text"
msgstr "(Shell Crew) Request an exchange with the Zoltan science team."

#: src-en/data/events_zoltan.xml.append:1306
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_CREW_STUDY\"]/choice[@req=\"LIST_CREW_SHELL\"]/event/text"
msgstr ""
"Overjoyed to come across this opportunity after having remained in Shell "
"space for so long, your Shell has many subjects to discuss, most of which "
"are beyond your understanding. Following a long debate and brainstorming "
"sessions with the Zoltans, they finally are able to return to you, and offer"
" a drone schematic in thanks."

#: src-en/data/events_zoltan.xml.append:1312
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_CREW_STUDY\"]/choice[@req=\"ANCIENT_CONSTRUCTOR\"]/text"
msgstr "(Hibernating Alien) Ask if they know what to do with this."

#: src-en/data/events_zoltan.xml.append:1314
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_CREW_STUDY\"]/choice[@req=\"ANCIENT_CONSTRUCTOR\"]/event/text"
msgstr ""
"\"Interesting... I have never seen a creature like this. I'm afraid there's "
"not much we can do with it however, its current state is unrelated to stasis"
" sleep. However, with a catalyst, such as quantum fuel, you may be able to "
"awaken it again?\""

#: src-en/data/events_zoltan.xml.append:1320
msgid ""
"data/events_zoltan.xml.append$//eventList[@name=\"ZOLTAN_CREW_STUDY_LIST\"]/event[1]/text"
msgstr ""
"Your crew calmly lines up for the Zoltans to take their readings. After a "
"short time, the process is done. They contact you, \"Thank you for your "
"participation in our study. Please accept these small cakes made from stiff "
"dough as well as some scrap.\""

#: src-en/data/events_zoltan.xml.append:1324
msgid ""
"data/events_zoltan.xml.append$//eventList[@name=\"ZOLTAN_CREW_STUDY_LIST\"]/event[2]/text"
msgstr ""
"Your crew calmly lines up for the Zoltans to take their readings. After a "
"short time, the process is done. They contact you, \"Thank you for your "
"participation in our study. Please accept these small cakes made from stiff "
"dough as well as some scrap.\""

#: src-en/data/events_zoltan.xml.append:1328
msgid ""
"data/events_zoltan.xml.append$//eventList[@name=\"ZOLTAN_CREW_STUDY_LIST\"]/event[3]/text"
msgstr ""
"As soon as you dock, pirates burst on board and a hostile ship appears on "
"the radar. You hear the Zoltans yell in the distance, \"We're being held "
"hostage!\""

#: src-en/data/events_zoltan.xml.append:1332
msgid ""
"data/events_zoltan.xml.append$//eventList[@name=\"ZOLTAN_CREW_STUDY_LIST\"]/event[3]/choice/text"
msgstr "Continue..."

#: src-en/data/events_zoltan.xml.append:1361
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"ENCOUNTER_ZOLTAN_ELITE\"]/text"
msgstr ""
"Though not all Zoltans choose to align with the extreme views of the "
"Duskbringers, that doesn't mean they all appreciate the Federation. A "
"particularly annoyed Zoltan Elite appears to be fed up with tolerating "
"Federation ships and has decided to attack."

#: src-en/data/events_zoltan.xml.append:1364
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"ENCOUNTER_ZOLTAN_ELITE\"]/choice/text"
msgstr "Continue..."

#: src-en/data/events_zoltan.xml.append:1373
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"ENCOUNTER_DUSKBRINGER_CAPITAL\"]/choice/text"
msgstr "Continue..."

#: src-en/data/events_zoltan.xml.append:1378
msgid ""
"data/events_zoltan.xml.append$//textList[@name=\"TEXT_ENCOUNTER_DUSK_CAPITAL\"]/text[1]"
msgstr ""
"A hostile Duskbringer ship intercepts you, spouting nonsense about the "
"inevitable fall of the Federation."

#: src-en/data/events_zoltan.xml.append:1379
msgid ""
"data/events_zoltan.xml.append$//textList[@name=\"TEXT_ENCOUNTER_DUSK_CAPITAL\"]/text[2]"
msgstr ""
"The Duskbringers have been gaining rapid support amongst the Zoltan "
"populace, so it's of no surprise you have discovered one of their ships at "
"this beacon."

#: src-en/data/events_zoltan.xml.append:1380
msgid ""
"data/events_zoltan.xml.append$//textList[@name=\"TEXT_ENCOUNTER_DUSK_CAPITAL\"]/text[3]"
msgstr ""
"\"Your last mistake was entering Zoltan territory, repugnant Federation "
"scum!\" An irate Duskbringer ship attacks."

#: src-en/data/events_zoltan.xml.append:1381
msgid ""
"data/events_zoltan.xml.append$//textList[@name=\"TEXT_ENCOUNTER_DUSK_CAPITAL\"]/text[4]"
msgstr ""
"It appears a Duskbringer ship has just finished off a Zoltan police ship, "
"and decides to target you next."

#: src-en/data/events_zoltan.xml.append:1388
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"ENCOUNTER_ROCK_CAPITAL\"]/choice/text"
msgstr "Continue..."

#: src-en/data/events_zoltan.xml.append:1393
msgid ""
"data/events_zoltan.xml.append$//textList[@name=\"TEXT_ENCOUNTER_ROCK_CAPITAL\"]/text[1]"
msgstr ""
"The Rockmen and Zoltans have been on good terms for longer than the "
"Federation has been around. Unfortunately, this means plenty of Rock ships "
"can be found here, and they usually aren't very friendly."

#: src-en/data/events_zoltan.xml.append:1394
msgid ""
"data/events_zoltan.xml.append$//textList[@name=\"TEXT_ENCOUNTER_ROCK_CAPITAL\"]/text[2]"
msgstr ""
"\"You have upset the gods by coming here, Federation Cruiser!\" You have no "
"idea what they are talking about, but a Rock ship visiting this Zoltan "
"sector approaches, weapons armed and ready."

#: src-en/data/events_zoltan.xml.append:1395
msgid ""
"data/events_zoltan.xml.append$//textList[@name=\"TEXT_ENCOUNTER_ROCK_CAPITAL\"]/text[3]"
msgstr ""
"A Rock ship swoops in to attack. It's no surprise to see a Rock ship in "
"Zoltan space, but it's unusual for them to be so aggressive without even a "
"reason."

#: src-en/data/events_zoltan.xml.append:1396
msgid ""
"data/events_zoltan.xml.append$//textList[@name=\"TEXT_ENCOUNTER_ROCK_CAPITAL\"]/text[4]"
msgstr ""
"It seems you're out of luck... a hostile Rock ship has already explored this"
" beacon and upon seeing you decides it wants to fight!"

#: src-en/data/events_zoltan.xml.append:1403
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"ENCOUNTER_REBEL_CAPITAL\"]/choice/text"
msgstr "Continue..."

#: src-en/data/events_zoltan.xml.append:1408
msgid ""
"data/events_zoltan.xml.append$//textList[@name=\"TEXT_ENCOUNTER_REBEL_CAPITAL\"]/text[1]"
msgstr ""
"\"I've been growing tired of these pompous energy freaks, a Fed ship will be"
" a nice change!\" A Rebel ship locks onto you and fires its entire salvo. "
"Man the battle-stations!"

#: src-en/data/events_zoltan.xml.append:1409
msgid ""
"data/events_zoltan.xml.append$//textList[@name=\"TEXT_ENCOUNTER_REBEL_CAPITAL\"]/text[2]"
msgstr ""
"Rebels are known for their xenophobia, but when it comes to the Zoltan race "
"they have an especially exaggerated hatred. A Rebel ship hunting for Zoltans"
" to kill spots you and decides to delay their search to attack you."

#: src-en/data/events_zoltan.xml.append:1410
msgid ""
"data/events_zoltan.xml.append$//textList[@name=\"TEXT_ENCOUNTER_REBEL_CAPITAL\"]/text[3]"
msgstr ""
"\"I suspected you might try to run here, hoping for protection by your "
"precious Zoltan allies! Well you were wrong to think you'd be safe here, "
"because I too have arrived!\""

#: src-en/data/events_zoltan.xml.append:1411
msgid ""
"data/events_zoltan.xml.append$//textList[@name=\"TEXT_ENCOUNTER_REBEL_CAPITAL\"]/text[4]"
msgstr ""
"A Rebel ship approaches fast! They must have known this was Federation-"
"allied territory and came to hunt for ships such as yours."

#: src-en/data/events_zoltan.xml.append:1418
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"ENCOUNTER_PIRATE_CAPITAL\"]/choice/text"
msgstr "Continue..."

#: src-en/data/events_zoltan.xml.append:1423
msgid ""
"data/events_zoltan.xml.append$//textList[@name=\"TEXT_ENCOUNTER_PIRATE_CAPITAL\"]/text[1]"
msgstr ""
"There's a tinge of irony how common pirates are in Zoltan territory despite "
"how hard their government tries to rid of them. Unfortunately, they usually "
"aren't happy to see your ship!"

#: src-en/data/events_zoltan.xml.append:1424
msgid ""
"data/events_zoltan.xml.append$//textList[@name=\"TEXT_ENCOUNTER_PIRATE_CAPITAL\"]/text[2]"
msgstr ""
"A pirate ship appears to be attempting to set up a small market here, but "
"when they spot you they ditch their efforts and in-turn attack!"

#: src-en/data/events_zoltan.xml.append:1425
msgid ""
"data/events_zoltan.xml.append$//textList[@name=\"TEXT_ENCOUNTER_PIRATE_CAPITAL\"]/text[3]"
msgstr ""
"A hostile pirate ship moves in to fight you, despite the Zoltan Peace-"
"Keepers that supposedly protect this sector trying desperately to clear it "
"of pirate forces."

#: src-en/data/events_zoltan.xml.append:1426
msgid ""
"data/events_zoltan.xml.append$//textList[@name=\"TEXT_ENCOUNTER_PIRATE_CAPITAL\"]/text[4]"
msgstr ""
"\"I wonder how much the Zoltans would pay me for as bounty for capturing an "
"illegal Multiverse vessel!\" A pirate ship swoops in, with malicious "
"thoughts on their mind."

#: src-en/data/events_zoltan.xml.append:1439
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"ENCOUNTER_ZOLTAN_AUTO_SUN\"]/text"
msgstr ""
"A Zoltan Automated ship appears to be in charging mode nearby this sun... "
"probably harvesting the solar energy! However, it appears you arrives just "
"in time for it to re-activate, and the ship somehow identifies you as a "
"threat."

#: src-en/data/events_zoltan.xml.append:1443
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"ENCOUNTER_ZOLTAN_AUTO_SUN\"]/choice/text"
msgstr "Continue..."

#: src-en/data/events_zoltan.xml.append:1449
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"ENCOUNTER_ZOLTAN_PULSAR\"]/text"
msgstr ""
"A crazed ship of Zoltans hails you, their raspy voices suggesting they've "
"gone mad with some sort of sickness. \"Hahahah! The energy of the pulsar, it"
" is mine! I shall become one with it! I shall become the pulsar! Die mortal,"
" be killed by my power!\""

#: src-en/data/events_zoltan.xml.append:1453
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"ENCOUNTER_ZOLTAN_PULSAR\"]/choice/text"
msgstr "Continue..."

#: src-en/data/events_zoltan.xml.append:1485
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_FREEMANTIS_FRIENDLY\"]/text"
msgstr ""
"The Free Mantis and Zoltan don't get along well, so it's concerning to see a"
" Free Mantis ship so far into Zoltan space. A ship from the peacekeeper "
"border police has docked with a massive Free Mantis ship, which can't mean "
"anything good."

#: src-en/data/events_zoltan.xml.append:1488
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_FREEMANTIS_FRIENDLY\"]/choice[1]/text"
msgstr "Board as well and see what's happening."

#: src-en/data/events_zoltan.xml.append:1490
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_FREEMANTIS_FRIENDLY\"]/choice[1]/event/text"
msgstr ""
"You step into the ship only to find a Peacekeeper violently kicking a "
"defenseless Free Mantis warlord on the ground, who is desperately screaming "
"\"Stop, stop! I not wanting violence, only friendlyness!\", but the Zoltan "
"won't listen."

#: src-en/data/events_zoltan.xml.append:1492
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_FREEMANTIS_FRIENDLY\"]/choice[1]/event/choice[1]/text"
msgstr "Pull the Zoltan off of the Free Mantis."

#: src-en/data/events_zoltan.xml.append:1494
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_FREEMANTIS_FRIENDLY\"]/choice[1]/event/choice[1]/event/text"
msgstr ""
"You grab at the Zoltan, pulling them away from the Free Mantis. They shove "
"you away, shouting \"What the hell are you doing? All your precious "
"Federation can do now is interfere with foreign matters, isn't that so? I'll"
" put you in your god damn place!\" The Zoltan gives orders for his men to "
"attack!"

#: src-en/data/events_zoltan.xml.append:1497
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_FREEMANTIS_FRIENDLY\"]/choice[1]/event/choice[1]/event/choice/text"
msgstr "Continue..."

#: src-en/data/events_zoltan.xml.append:1503
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_FREEMANTIS_FRIENDLY\"]/choice[1]/event/choice[2]/text"
msgstr "Try to reason with the Zoltan."

#: src-en/data/events_zoltan.xml.append:1505
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_FREEMANTIS_FRIENDLY\"]/choice[1]/event/choice[2]/event/text"
msgstr ""
"You try to reason with the Peacekeeper, but it only infuriates them further."
" \"The Federation should know their place, instead of always interfering "
"with foreign matters. Maybe the Duskbringers are right, and all of you filth"
" deserve to die.\" The Peacekeeper gives orders for his men to attack!"

#: src-en/data/events_zoltan.xml.append:1508
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_FREEMANTIS_FRIENDLY\"]/choice[1]/event/choice[2]/event/choice/text"
msgstr "Continue..."

#: src-en/data/events_zoltan.xml.append:1514
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_FREEMANTIS_FRIENDLY\"]/choice[1]/event/choice[@req=\"LIST_CREW_ZOLTAN_CAPITAL\"]/text"
msgstr "(Ministry Allied Zoltan Crew) Have your Zoltan reason with them."

#: src-en/data/events_zoltan.xml.append:1518
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_FREEMANTIS_FRIENDLY\"]/choice[1]/event/choice[@req=\"ZOLTAN_DELETER\"]/text"
msgstr "(Zoltan Deleter) Begone."

#: src-en/data/events_zoltan.xml.append:1520
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_FREEMANTIS_FRIENDLY\"]/choice[1]/event/choice[@req=\"ZOLTAN_DELETER\"]/event/text"
msgstr ""
"It only takes a single shot to erase the peace-keeper from existence. You "
"help the Free Mantis to their feet. \"Thanking you very much Federation, we "
"come here only make friends, but Zoltan not trust us. Me coming with you, "
"much friendlier.\""

#: src-en/data/events_zoltan.xml.append:1527
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_FREEMANTIS_FRIENDLY\"]/choice[2]/text"
msgstr "Ignore the ships."

#: src-en/data/events_zoltan.xml.append:1529
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_FREEMANTIS_FRIENDLY\"]/choice[2]/event/text"
msgstr ""
"There's no way you're messing with the Zoltan border police in the Zoltan "
"Capital."

#: src-en/data/events_zoltan.xml.append:1531
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_FREEMANTIS_FRIENDLY\"]/choice[2]/event/choice[1]/text"
msgstr "Do something onboard the ship."

#: src-en/data/events_zoltan.xml.append:1535
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_FREEMANTIS_FRIENDLY\"]/choice[2]/event/choice[2]/text"
msgstr "Do nothing."

#: src-en/data/events_zoltan.xml.append:1542
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_FREEMANTIS_FRIENDLY_REASON_ZOLTAN\"]/text"
msgstr ""
"\"What are you doing? Is this really keeping the peace? The Ministry stands "
"for lawful justice, and yet here you are attacking a defenseless Mantis! Is "
"this what you signed up to the force for?\" The Peacekeeper falters for a "
"moment. \"I- don't try and get involved in things that have nothing to do "
"with you, Federation!\" The Zoltan begins to say more, but is immediately "
"sliced in twain by the Free Mantis, who crawled off the floor while the "
"Zoltan was distracted. You hurry back to the ship before the Peacekeeper "
"ship notices something is wrong, but it's too late. You escape from the "
"ship, but a team of Zoltan beam aboard your ship!"

#: src-en/data/events_zoltan.xml.append:1546
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_FREEMANTIS_FRIENDLY_REASON_ZOLTAN\"]/choice/text"
msgstr "Continue..."

#: src-en/data/events_zoltan.xml.append:1552
msgid "data/events_zoltan.xml.append$//event[@name=\"CAPITAL_REVOLT\"]/text"
msgstr ""
"Despite the arrogant nature of the Zoltans regarding their supposedly "
"superior government, many of their kind are dissatisfied with the strict and"
" controlling Zoltan Peace-Keepers instated to, as their name would suggest, "
"'keep the peace'. It appears a full on riot has started on the nearby "
"planet."

#: src-en/data/events_zoltan.xml.append:1555
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_REVOLT\"]/choice[1]/text"
msgstr "Pull closer to survey the chaos."

#: src-en/data/events_zoltan.xml.append:1557
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_REVOLT\"]/choice[1]/event/text"
msgstr ""
"It seems the conflict was incited by a Duskbringer soldier giving a rather "
"motivating speech, which inevitably resulted in complete and utter "
"destruction, as civilians overturn Peace-Keeper ground vehicles and orbital "
"satellites open fire on crowds of innocent people trying to flee for their "
"life."

#: src-en/data/events_zoltan.xml.append:1559
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_REVOLT\"]/choice[1]/event/choice[1]/text"
msgstr "Take part in the battle - help the Zoltan civilians."

#: src-en/data/events_zoltan.xml.append:1561
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_REVOLT\"]/choice[1]/event/choice[1]/event/text"
msgstr ""
"You've made up your mind to help the civilians. You attack one of the Peace-"
"Keeper vessels."

#: src-en/data/events_zoltan.xml.append:1564
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_REVOLT\"]/choice[1]/event/choice[1]/event/choice/text"
msgstr "Continue..."

#: src-en/data/events_zoltan.xml.append:1570
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_REVOLT\"]/choice[1]/event/choice[2]/text"
msgstr "Take part in the battle - help the Peace-Keepers."

#: src-en/data/events_zoltan.xml.append:1572
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_REVOLT\"]/choice[1]/event/choice[2]/event/text"
msgstr ""
"You've made up your mind to help the Peace-Keepers. You attack one of the "
"Duskbringer vessels."

#: src-en/data/events_zoltan.xml.append:1575
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_REVOLT\"]/choice[1]/event/choice[2]/event/choice/text"
msgstr "Continue..."

#: src-en/data/events_zoltan.xml.append:1581
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_REVOLT\"]/choice[1]/event/choice[@req=\"cloaking\"]/text"
msgstr ""
"(Cloaking) Cloak and loot some of the buildings while everyone is "
"distracted."

#: src-en/data/events_zoltan.xml.append:1583
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_REVOLT\"]/choice[1]/event/choice[@req=\"cloaking\"]/event/text"
msgstr ""
"You swoop down to collect some supplies as quickly as possible before "
"leaving."

#: src-en/data/events_zoltan.xml.append:1588
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_REVOLT\"]/choice[1]/event/choice[4]/text"
msgstr "Pull away quickly."

#: src-en/data/events_zoltan.xml.append:1594
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_REVOLT\"]/choice[2]/text"
msgstr "This isn't our business, ignore them."

#: src-en/data/events_zoltan.xml.append:1596
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_REVOLT\"]/choice[2]/event/text"
msgstr ""
"It's not your duty to get involved in the business of everyone else you "
"encounter. You ignore the chaos below."

#: src-en/data/events_zoltan.xml.append:1598
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_REVOLT\"]/choice[2]/event/choice[1]/text"
msgstr "Do something onboard the ship."

#: src-en/data/events_zoltan.xml.append:1602
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_REVOLT\"]/choice[2]/event/choice[2]/text"
msgstr "Do nothing."

#: src-en/data/events_zoltan.xml.append:1610
msgid ""
"data/events_zoltan.xml.append$//eventList[@name=\"CAPITAL_REVOLT_CIVILIANS_HELP\"]/event[1]/text"
msgstr ""
"The civilians manage to fight back the Peace-Keeper forces, who flee from "
"the scene before more casualties can occur. It appears your efforts did not "
"go unnoticed either, and the civilians bring you a large reward for your "
"assistance."

#: src-en/data/events_zoltan.xml.append:1614
msgid ""
"data/events_zoltan.xml.append$//eventList[@name=\"CAPITAL_REVOLT_CIVILIANS_HELP\"]/event[2]/text"
msgstr ""
"The civilians manage to fight back the Peace-Keeper forces, who flee from "
"the scene before more casualties can occur. It appears your efforts did not "
"go unnoticed either, and the civilians bring you a large reward for your "
"assistance."

#: src-en/data/events_zoltan.xml.append:1618
msgid ""
"data/events_zoltan.xml.append$//eventList[@name=\"CAPITAL_REVOLT_CIVILIANS_HELP\"]/event[3]/text"
msgstr ""
"Unfortunately, it seems your efforts were in vain. The civilians are "
"defeated and the Peace-Keepers retake the city. You flee before they can "
"chase you off themselves."

#: src-en/data/events_zoltan.xml.append:1623
msgid ""
"data/events_zoltan.xml.append$//eventList[@name=\"CAPITAL_REVOLT_PEACE_HELP\"]/event[1]/text"
msgstr ""
"As you expected, the Peace-Keepers quickly retake the sector from the "
"Duskbringers and begin making arrests. It appears you chose to help the "
"right side, because the Peace-Keepers have taken note of your efforts at "
"assisting them and pay you in scrap."

#: src-en/data/events_zoltan.xml.append:1627
msgid ""
"data/events_zoltan.xml.append$//eventList[@name=\"CAPITAL_REVOLT_PEACE_HELP\"]/event[2]/text"
msgstr ""
"It appears your efforts were in vain, as the Duskbringer forces beat back "
"the Peace-Keepers and slowly take control over the city. As the Capital "
"forces flee, a lone soldier runs to your ship and offers to join you so they"
" can escape."

#: src-en/data/events_zoltan.xml.append:1631
msgid ""
"data/events_zoltan.xml.append$//eventList[@name=\"CAPITAL_REVOLT_PEACE_HELP\"]/event[3]/text"
msgstr ""
"While scrapping the hull of the Duskbringer ship, a massive explosion erupts"
" from one of the nearby buildings! Your ship takes some damage but luckily "
"you pull away in time before it can be compromised. Another series of "
"explosions rocks the city, as buildings crumble to the ground! A squadron of"
" Duskbringer bombs descend from the sky, reducing the city to ashes. You fly"
" away as quickly as possible."

#: src-en/data/events_zoltan.xml.append:1650
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"DISTRESS_CAPITAL_ARREST\"]/text"
msgstr ""
"It appears a pirate ship here is under arrest by a Zoltan ship. They've "
"secretly activated the distress beacon, and have sent you a short message "
"saying \"help\"."

#: src-en/data/events_zoltan.xml.append:1652
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"DISTRESS_CAPITAL_ARREST\"]/choice[1]/text"
msgstr "Assist the pirates and attack the Zoltans."

#: src-en/data/events_zoltan.xml.append:1654
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"DISTRESS_CAPITAL_ARREST\"]/choice[1]/event/text"
msgstr "You arm the weapons and engage the Zoltan vessel."

#: src-en/data/events_zoltan.xml.append:1657
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"DISTRESS_CAPITAL_ARREST\"]/choice[1]/event/choice/text"
msgstr "Continue..."

#: src-en/data/events_zoltan.xml.append:1663
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"DISTRESS_CAPITAL_ARREST\"]/choice[2]/text"
msgstr "Report the pirates to the Zoltans."

#: src-en/data/events_zoltan.xml.append:1665
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"DISTRESS_CAPITAL_ARREST\"]/choice[2]/event/text"
msgstr ""
"The Zoltans gruffly thank you and give you some fuel, then go back to "
"dealing with the pirates."

#: src-en/data/events_zoltan.xml.append:1670
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"DISTRESS_CAPITAL_ARREST\"]/choice[3]/text"
msgstr "Ignore the pirates."

#: src-en/data/events_zoltan.xml.append:1672
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"DISTRESS_CAPITAL_ARREST\"]/choice[3]/event/text"
msgstr ""
"The pirates send you no future messages and are all brought aboard the "
"Zoltan ship, which jumps away shortly after."

#: src-en/data/events_zoltan.xml.append:1674
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"DISTRESS_CAPITAL_ARREST\"]/choice[3]/event/choice[1]/text"
msgstr "Do something onboard the ship."

#: src-en/data/events_zoltan.xml.append:1678
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"DISTRESS_CAPITAL_ARREST\"]/choice[3]/event/choice[2]/text"
msgstr "Do nothing."

#: src-en/data/events_zoltan.xml.append:1686
msgid ""
"data/events_zoltan.xml.append$//eventList[@name=\"DISTRESS_CAPITAL_ARREST_WIN\"]/event[1]/text"
msgstr ""
"It appears the pirates were slavers. Grateful for your assistance, they "
"offer you one of their 'workers' for free."

#: src-en/data/events_zoltan.xml.append:1690
msgid ""
"data/events_zoltan.xml.append$//eventList[@name=\"DISTRESS_CAPITAL_ARREST_WIN\"]/event[2]/text"
msgstr ""
"The pirates appear to be mercenaries, ironically hired to hunt you down by "
"the Rebels. After an awkward silence, you work out a deal, and they promise "
"to lie to the fleet."

#: src-en/data/events_zoltan.xml.append:1694
msgid ""
"data/events_zoltan.xml.append$//eventList[@name=\"DISTRESS_CAPITAL_ARREST_WIN\"]/event[3]/text"
msgstr ""
"The pirates appear to be smugglers. They explain that their client snitched "
"on them and led to their arrest. Now that you've saved them, they offer to "
"sell to you instead."

#: src-en/data/events_zoltan.xml.append:1700
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"DISTRESS_CAPITAL_RAID\"]/text"
msgstr ""
"A Zoltan Peace Brigade is under heavy fire at this beacon by a battalion of "
"Rebel fighters. The distress beacon comes from the commanding ship. They "
"appear to be too busy to notice you."

#: src-en/data/events_zoltan.xml.append:1703
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"DISTRESS_CAPITAL_RAID\"]/choice[1]/text"
msgstr "Hail them and offer assistance."

#: src-en/data/events_zoltan.xml.append:1705
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"DISTRESS_CAPITAL_RAID\"]/choice[1]/event/text"
msgstr ""
"It's hard to hear the Zoltan commander over the sounds of gunfire on their "
"ship. \"What?! Oh, you've come to provide assistance? We need all the help "
"we can, there's numerous Rebel ships attacking this system! If you're going "
"to help, be quick about it!\""

#: src-en/data/events_zoltan.xml.append:1707
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"DISTRESS_CAPITAL_RAID\"]/choice[1]/event/choice[1]/text"
msgstr "Attack one of the smaller Rebel ships."

#: src-en/data/events_zoltan.xml.append:1709
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"DISTRESS_CAPITAL_RAID\"]/choice[1]/event/choice[1]/event/text"
msgstr ""
"Several Rebel ships are scattered around the perimeter, swooping in to "
"assist their comrades when necessary. You pick a ship at random and attack."

#: src-en/data/events_zoltan.xml.append:1712
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"DISTRESS_CAPITAL_RAID\"]/choice[1]/event/choice[1]/event/choice/text"
msgstr "Continue..."

#: src-en/data/events_zoltan.xml.append:1718
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"DISTRESS_CAPITAL_RAID\"]/choice[1]/event/choice[2]/text"
msgstr "Take on one of the Elite ships."

#: src-en/data/events_zoltan.xml.append:1720
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"DISTRESS_CAPITAL_RAID\"]/choice[1]/event/choice[2]/event/text"
msgstr ""
"The burden of the attack is being carried out by Elite vessels. You "
"intercept one of them and attack."

#: src-en/data/events_zoltan.xml.append:1723
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"DISTRESS_CAPITAL_RAID\"]/choice[1]/event/choice[2]/event/choice/text"
msgstr "Continue..."

#: src-en/data/events_zoltan.xml.append:1729
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"DISTRESS_CAPITAL_RAID\"]/choice[1]/event/choice[3]/text"
msgstr "Take on the ship leading the squadron."

#: src-en/data/events_zoltan.xml.append:1731
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"DISTRESS_CAPITAL_RAID\"]/choice[1]/event/choice[3]/event/text"
msgstr ""
"A powerful MFK Ace is leading the Rebels. If you can destroy it, the Zoltans"
" might be able to win this battle."

#: src-en/data/events_zoltan.xml.append:1734
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"DISTRESS_CAPITAL_RAID\"]/choice[1]/event/choice[3]/event/choice/text"
msgstr "Continue..."

#: src-en/data/events_zoltan.xml.append:1742
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"DISTRESS_CAPITAL_RAID\"]/choice[2]/text"
msgstr "This is chaos, leave."

#: src-en/data/events_zoltan.xml.append:1747
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_RAID_BORING_WIN\"]/text"
msgstr ""
"You salvage the remains of the Rebel ship while the battle rages on in the "
"distance."

#: src-en/data/events_zoltan.xml.append:1750
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_RAID_BORING_WIN\"]/choice/text"
msgstr "Continue..."

#: src-en/data/events_zoltan.xml.append:1752
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_RAID_BORING_WIN\"]/choice/event/text"
msgstr ""
"The Zoltans are too busy dealing with the conflict to reward you, so you "
"decide it's a good time to leave before another Rebel ship attacks you."

#: src-en/data/events_zoltan.xml.append:1757
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_RAID_ELITE_WIN\"]/text"
msgstr ""
"You salvage the remains of the Elite Rebel ship while the battle rages on in"
" the distance."

#: src-en/data/events_zoltan.xml.append:1760
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_RAID_ELITE_WIN\"]/choice/text"
msgstr "Continue..."

#: src-en/data/events_zoltan.xml.append:1762
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_RAID_ELITE_WIN\"]/choice/event/text"
msgstr ""
"The Zoltans are too busy dealing with the conflict to reward you, so you "
"decide it's a good time to leave before another Rebel ship attacks you."

#: src-en/data/events_zoltan.xml.append:1767
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_RAID_SELITE_WIN\"]/text"
msgstr ""
"The MFK Ace has been neutralized. You salvage the remains of the ship."

#: src-en/data/events_zoltan.xml.append:1770
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_RAID_SELITE_WIN\"]/choice/text"
msgstr "Continue..."

#: src-en/data/events_zoltan.xml.append:1772
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_RAID_SELITE_WIN\"]/choice/event/text"
msgstr ""
"Around you the battle begins to die down. The Rebels, watching you defeat "
"their leader, reconsider and begin to jump away. In a matter of minutes the "
"Zoltan forces go from near defeat to conclusive victory."

#: src-en/data/events_zoltan.xml.append:1774
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_RAID_SELITE_WIN\"]/choice/event/choice/text"
msgstr "Contact the Zoltans."

#: src-en/data/events_zoltan.xml.append:1776
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"CAPITAL_RAID_SELITE_WIN\"]/choice/event/choice/event/text"
msgstr ""
"\"You have saved us from a terrible defeat. We are grateful for your "
"assistance. Allow me to repay our debt to you.\""

#: src-en/data/events_zoltan.xml.append:1797
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"QUEST_CAPITAL_LANIUS_CORE\"]/text"
msgstr ""
"To your surprise, this beacon is inhabited by a Lanius vessel! They don't "
"attack, but instead send you a message."

#: src-en/data/events_zoltan.xml.append:1800
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"QUEST_CAPITAL_LANIUS_CORE\"]/choice[1]/text"
msgstr "Respond."

#: src-en/data/events_zoltan.xml.append:1802
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"QUEST_CAPITAL_LANIUS_CORE\"]/choice[1]/event/text"
msgstr ""
"\"Offer asking. Green! Force want. Core Zoltan collect, bring us.\" It's "
"hard to understand, but after further discussion it seems the Lanius are "
"interested in the Zoltan shields on this sector's inhabitants, and are "
"asking you for a favor. Capture a Zoltan ship and bring to the Lanius their "
"shield's core, and they will pay you."

#: src-en/data/events_zoltan.xml.append:1804
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"QUEST_CAPITAL_LANIUS_CORE\"]/choice[1]/event/choice[1]/text"
msgstr "Agree."

#: src-en/data/events_zoltan.xml.append:1806
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"QUEST_CAPITAL_LANIUS_CORE\"]/choice[1]/event/choice[1]/event/text"
msgstr ""
"\"Greater. Location sending.\" The Lanius attach the co-ordinates of the "
"ship they are wanting the core from."

#: src-en/data/events_zoltan.xml.append:1811
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"QUEST_CAPITAL_LANIUS_CORE\"]/choice[1]/event/choice[2]/text"
msgstr "Decline."

#: src-en/data/events_zoltan.xml.append:1817
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"QUEST_CAPITAL_LANIUS_CORE\"]/choice[2]/text"
msgstr "Attack!"

#: src-en/data/events_zoltan.xml.append:1819
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"QUEST_CAPITAL_LANIUS_CORE\"]/choice[2]/event/text"
msgstr "If the Lanius aren't going to attack, then you will!"

#: src-en/data/events_zoltan.xml.append:1822
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"QUEST_CAPITAL_LANIUS_CORE\"]/choice[2]/event/choice/text"
msgstr "Continue..."

#: src-en/data/events_zoltan.xml.append:1828
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"QUEST_CAPITAL_LANIUS_CORE\"]/choice[3]/text"
msgstr "Ignore the Lanius."

#: src-en/data/events_zoltan.xml.append:1834
msgid ""
"data/events_zoltan.xml.append$//eventList[@name=\"QUEST_CAPITAL_LANIUS_CORE_IGNORE\"]/event[1]/text"
msgstr ""
"The Lanius eventually jump away after realizing you don't intend on "
"responding."

#: src-en/data/events_zoltan.xml.append:1836
msgid ""
"data/events_zoltan.xml.append$//eventList[@name=\"QUEST_CAPITAL_LANIUS_CORE_IGNORE\"]/event[1]/choice[1]/text"
msgstr "Do something onboard the ship."

#: src-en/data/events_zoltan.xml.append:1840
msgid ""
"data/events_zoltan.xml.append$//eventList[@name=\"QUEST_CAPITAL_LANIUS_CORE_IGNORE\"]/event[1]/choice[2]/text"
msgstr "Do nothing."

#: src-en/data/events_zoltan.xml.append:1845
msgid ""
"data/events_zoltan.xml.append$//eventList[@name=\"QUEST_CAPITAL_LANIUS_CORE_IGNORE\"]/event[2]/text"
msgstr ""
"The Lanius aren't pleased by being ignored... they arm their weapons and "
"attack!"

#: src-en/data/events_zoltan.xml.append:1848
msgid ""
"data/events_zoltan.xml.append$//eventList[@name=\"QUEST_CAPITAL_LANIUS_CORE_IGNORE\"]/event[2]/choice/text"
msgstr "Continue..."

#: src-en/data/events_zoltan.xml.append:1861
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"QUEST_CAPITAL_LANIUS_CORE_REGULAR\"]/text"
msgstr ""
"The co-ordinates have led you to a regular Zoltan ship. If you want the "
"core, you'll have to clear them of inhabitants first."

#: src-en/data/events_zoltan.xml.append:1864
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"QUEST_CAPITAL_LANIUS_CORE_REGULAR\"]/choice[1]/text"
msgstr "Attack!"

#: src-en/data/events_zoltan.xml.append:1866
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"QUEST_CAPITAL_LANIUS_CORE_REGULAR\"]/choice[1]/event/text"
msgstr "You arm the weapons and approach the Zoltan ship."

#: src-en/data/events_zoltan.xml.append:1869
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"QUEST_CAPITAL_LANIUS_CORE_REGULAR\"]/choice[1]/event/choice/text"
msgstr "Continue..."

#: src-en/data/events_zoltan.xml.append:1875
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"QUEST_CAPITAL_LANIUS_CORE_REGULAR\"]/choice[2]/text"
msgstr "These Zoltan haven't done anything to us, leave them alone."

#: src-en/data/events_zoltan.xml.append:1877
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"QUEST_CAPITAL_LANIUS_CORE_REGULAR\"]/choice[2]/event/text"
msgstr "You decide not to fight the Zoltan."

#: src-en/data/events_zoltan.xml.append:1879
msgid ""
"data/events_zoltan.xml.append$//event[@name=\"QUEST_CAPITAL_LANIUS_CORE_REGULAR\"]/choice[2]/event/choice[1]/text"
msgstr "Do something onboard the ship."

#: src-en/data/events_zoltan.xml.append:1883
