#: src-en/data/blueprints.xml.append:10087
msgid ""
"data/blueprints.xml.append$//augBlueprint[@name=\"DISTRACTION_BUOY\"]/title"
msgstr "Bouées de diversion"

#: src-en/data/blueprints.xml.append:10088
msgid "data/blueprints.xml.append$//augBlueprint[@name=\"DISTRACTION_BUOY\"]/desc"
msgstr ""
"Permet le déploiement de bouées durant la transition entre secteurs. Peut "
"donner 1 délai de flotte gratuitement, ou déployer des bouées additionnelles"
" en échange de pièces de drones."

#: src-en/data/blueprints.xml.append:10096
msgid "data/blueprints.xml.append$//augBlueprint[@name=\"PRE_SCANNER\"]/title"
msgstr "Scanner supraluminique"

#: src-en/data/blueprints.xml.append:10097
msgid "data/blueprints.xml.append$//augBlueprint[@name=\"PRE_SCANNER\"]/desc"
msgstr ""
