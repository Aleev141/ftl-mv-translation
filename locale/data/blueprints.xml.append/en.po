msgid ""
"data/blueprints.xml.append$/FTL/FTL[10]/FTL[11]/FTL[2]/shipBlueprint[@name=\"PLAYER_SHIP_ALKRAM_2\"]/class"
msgstr "Revolutionary Cruiser"

