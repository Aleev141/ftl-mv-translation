#: src-en/data/blueprints.xml.append:57880
msgid ""
"data/blueprints.xml.append$/FTL/FTL[10]/FTL[13]/FTL[4]/shipBlueprint[@name=\"PLAYER_SHIP_GHOST_3\"]/class"
msgstr "Spook Chaser Cruiser"

#: src-en/data/blueprints.xml.append:57883
msgid ""
"data/blueprints.xml.append$/FTL/FTL[10]/FTL[13]/FTL[4]/shipBlueprint[@name=\"PLAYER_SHIP_GHOST_3\"]/desc"
msgstr ""
"This model of Spook Chaser ship was re-acquired by the Rebels. Instead of "
"tracking Ghosts, it was repurposed to hunt particular Multiverse Renegades."

#: src-en/data/blueprints.xml.append:57967
msgid ""
"data/blueprints.xml.append$/FTL/FTL[10]/FTL[13]/FTL[5]/shipBlueprint[@name=\"PLAYER_SHIP_GUARD\"]/class"
msgstr "Guardian Cruiser"

#: src-en/data/blueprints.xml.append:57969
msgid ""
"data/blueprints.xml.append$/FTL/FTL[10]/FTL[13]/FTL[5]/shipBlueprint[@name=\"PLAYER_SHIP_GUARD\"]/desc"
msgstr ""
"They're coming soon. Maybe you should think twice about opening the door."

#: src-en/data/blueprints.xml.append:58044
msgid ""
"data/blueprints.xml.append$/FTL/FTL[10]/FTL[13]/FTL[5]/shipBlueprint[@name=\"PLAYER_SHIP_GUARD_2\"]/class"
