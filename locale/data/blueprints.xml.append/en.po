msgstr "Geniocracy Science Cruiser"

#: src-en/data/blueprints.xml.append:54533
msgid ""
"data/blueprints.xml.append$/FTL/FTL[10]/FTL[10]/FTL[2]/shipBlueprint[@name=\"PLAYER_SHIP_SCIENTIST\"]/desc"
