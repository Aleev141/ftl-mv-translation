msgid ""
"data/blueprints.xml.append$/FTL/FTL[10]/FTL[14]/shipBlueprint[@name=\"CREW_SHIP_SLOT7_2\"]/desc"
msgstr ""
"The golden cruiser of the Crystal Empire's foreign ambassador himself. Due "
"to a chronic phobia induced from past life experiences, this ship has been "
"kept deliberately stasis-pod free."

#: src-en/data/blueprints.xml.append:60040
msgid ""
"data/blueprints.xml.append$/FTL/FTL[10]/FTL[14]/shipBlueprint[@name=\"CREW_SHIP_SLOT7_3\"]/class"
msgstr "Tuco's Crewser"

