msgstr "Haynes's Crewser"

#: src-en/data/blueprints.xml.append:58707
msgid ""
"data/blueprints.xml.append$/FTL/FTL[10]/FTL[14]/shipBlueprint[@name=\"CREW_SHIP_SLOT1\"]/desc"
