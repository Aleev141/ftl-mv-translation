#: src-en/data/events_storage.xml:30369
msgid ""
"data/events_storage.xml$//textList[@name=\"TEXT_CREW_TALK_BISHOP\"]/text[2]"
msgstr ""
"Your Bishop has recently learned of the human tradition of \"body pillows\" "
"and seeks to obtain one of Queen Leikonpliiinakra."

#: src-en/data/events_storage.xml:30370
msgid ""
"data/events_storage.xml$//textList[@name=\"TEXT_CREW_TALK_BISHOP\"]/text[3]"
msgstr ""
