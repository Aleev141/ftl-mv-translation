#: src-en/data/events_ancient.xml:1946
msgid ""
"data/events_ancient.xml$//event[@name=\"ROYAL_ROOMBA\"]/choice[2]/event/text"
msgstr "你经过自动飞船和哨站，继续赶你的路。"

#: src-en/data/events_ancient.xml:1948
msgid ""
"data/events_ancient.xml$//event[@name=\"ROYAL_ROOMBA\"]/choice[2]/event/choice[1]/text"
msgstr "星舰管理。"

#: src-en/data/events_ancient.xml:1952
