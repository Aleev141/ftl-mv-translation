msgstr "Pas grave."

#: src-en/data/events_piracy.xml:214
msgid ""
"data/events_piracy.xml$//event[@name=\"SLAVER_OFFERS\"]/choice[4]/event/text"
msgstr "Vous n'êtes pas intéressé par leur secteur d'activité."

#: src-en/data/events_piracy.xml:216
msgid ""
"data/events_piracy.xml$//event[@name=\"SLAVER_OFFERS\"]/choice[4]/event/choice[1]/text"
msgstr "Faire quelque chose à bord du vaisseau."

#: src-en/data/events_piracy.xml:220
