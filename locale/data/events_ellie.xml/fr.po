#: src-en/data/events_ellie.xml:77
msgid ""
"data/events_ellie.xml$//event[@name=\"ELLIE_START\"]/choice[1]/event/choice/event/choice[@req=\"unique_alkali\"]/event/choice[2]/event/text"
msgstr ""
