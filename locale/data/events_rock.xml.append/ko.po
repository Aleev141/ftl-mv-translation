#: src-en/data/events_rock.xml.append:566
msgid ""
"data/events_rock.xml.append$/FTL/event[@name=\"ROCK_WEAPON_MEASURING\"]/choice[2]/event/text"
msgstr "\"Hah! You call that big weapons? Your brain must be even smaller!\""

#: src-en/data/events_rock.xml.append:568
msgid ""
"data/events_rock.xml.append$/FTL/event[@name=\"ROCK_WEAPON_MEASURING\"]/choice[2]/event/choice[1]/text"
msgstr "(Lvl 4 Weapons) What about this?"

#: src-en/data/events_rock.xml.append:570
msgid ""
"data/events_rock.xml.append$/FTL/event[@name=\"ROCK_WEAPON_MEASURING\"]/choice[2]/event/choice[1]/event/text"
msgstr "\"Getting warmer, but that's still nothing compared to what we have!\""

#: src-en/data/events_rock.xml.append:572
msgid ""
"data/events_rock.xml.append$/FTL/event[@name=\"ROCK_WEAPON_MEASURING\"]/choice[2]/event/choice[1]/event/choice[1]/text"
