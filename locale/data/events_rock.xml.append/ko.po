msgid ""
"data/events_rock.xml.append$/FTL/event[@name=\"ROCK_WEAPON_MEASURING\"]/choice[5]/event/text"
msgstr "You ignore the egotistical Rockmen and continue on your way."

#: src-en/data/events_rock.xml.append:652
msgid ""
"data/events_rock.xml.append$/FTL/event[@name=\"ROCK_WEAPON_MEASURING\"]/choice[5]/event/choice[1]/text"
