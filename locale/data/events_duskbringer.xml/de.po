#: src-en/data/events_duskbringer.xml:1108
msgid ""
"data/events_duskbringer.xml$//event[@name=\"DUSKCAP_PRISONERS\"]/choice[@req=\"cloaking\"]/text"
msgstr "(Tarnung) Tarne dich und sende deine Crew unbemerkt rein."

#: src-en/data/events_duskbringer.xml:1112
